# 互利微信 
server {
        listen       80;
        server_name  duodian.m.dangqukeji.cn  ;

        proxy_set_header Host $host;
        proxy_set_header X-Real-IP $remote_addr;
        proxy_set_header REMOTE-HOST $remote_addr;
        proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;


        location / {
          proxy_pass http://proxy.m.fqapps.com;
        }
}


server {
        listen       80 ;
        server_name     duodian.cai.dangqukeji.cn ;

        proxy_set_header Host $host;
        proxy_set_header X-Real-IP $remote_addr;
        proxy_set_header REMOTE-HOST $remote_addr;
        proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;

        return 301 http://graceway.m.huoyingcangbaoge.cn$request_uri;
       
 }


server {
        listen       80;
        server_name  kequyh.m.dangquyouhui.cn ;

        proxy_set_header Host $host;
        proxy_set_header X-Real-IP $remote_addr;
        proxy_set_header REMOTE-HOST $remote_addr;
        proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;

       location = /root.txt {
            default_type text/html;
            return 200 '32f9262143d2479080df590b3cff55f4';  
        }

        location / {
          proxy_pass http://proxy.m.fqapps.com;
        }
}




#web-h5
server {
        listen       80;
        server_name  web.duodianapp.cn web.huoyingcangbaoge.cn;

        root        /usr/local/web-h5/;
        index       index.html,index.htm;

        proxy_set_header Host $host;
        proxy_set_header X-Real-IP $remote_addr;
        proxy_set_header REMOTE-HOST $remote_addr;
        proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;
     
        location / {
            try_files $uri /index.html;
        }

}

server {
        listen       80;
        server_name  estimate.latl.site;

        root        /usr/local/estimate/;
        index       index.html,index.htm;

        proxy_set_header Host $host;
        proxy_set_header X-Real-IP $remote_addr;
        proxy_set_header REMOTE-HOST $remote_addr;
        proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;
     
        location / {
            try_files $uri /index.html;
        }

}






