server {
        listen       80;
        server_name  xiaodangserver.duoquwawa.com;

	    proxy_set_header Host $host;
        proxy_set_header X-Real-IP $remote_addr;
        proxy_set_header REMOTE-HOST $remote_addr;
        proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;

	    
	    location / {
	    	proxy_pass http://47.104.239.245:8081/;
        }
}

server {
    listen       80;
    server_name  xiaodangserver.duoquwawa.cn;

    proxy_set_header Host $host;
    proxy_set_header X-Real-IP $remote_addr;
    proxy_set_header REMOTE-HOST $remote_addr;
    proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;
        
    location / {
        proxy_pass http://47.104.239.245:8081/;
    }
}

server {
        listen       80;
        server_name  xiaodangquan.duoquwawa.com;

        root        /usr/local/VueWebProject/;
        index      index.html,index.htm;

        proxy_set_header Host $host;
        proxy_set_header X-Real-IP $remote_addr;
        proxy_set_header REMOTE-HOST $remote_addr;
        proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;

     
     	location / {
            try_files $uri /index.html;
        }
}


server {
        listen       80;
        server_name  xiaogdanginfo.duoquwawa.com;

        root        /usr/local/VueWebInfoProject/;
        index       index.html,index.htm;

        proxy_set_header Host $host;
        proxy_set_header X-Real-IP $remote_addr;
        proxy_set_header REMOTE-HOST $remote_addr;
        proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;
     
        location / {
            try_files $uri /index.html;
        }
}






