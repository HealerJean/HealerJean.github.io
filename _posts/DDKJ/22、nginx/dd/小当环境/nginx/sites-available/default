
#小当后台运营前端
server {
        listen       80;
        server_name  xiaodangquan.duoquwawa.com;

        root        /usr/local/VueWebProject/;
        index      index.html,index.htm;

        proxy_set_header Host $host;
        proxy_set_header X-Real-IP $remote_addr;
        proxy_set_header REMOTE-HOST $remote_addr;
        proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;

     
     	location / {
            try_files $uri /index.html;
        }
}

#小当H5前端 
server {
        listen       80;
        server_name  xiaogdanginfo.duoquwawa.com infoxiaodang.nongfusiquan.cn ;

        root        /usr/local/VueWebInfoProject/;
        index       index.html,index.htm;

        proxy_set_header Host $host;
        proxy_set_header X-Real-IP $remote_addr;
        proxy_set_header REMOTE-HOST $remote_addr;
        proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;
     
        location / {
            try_files $uri /index.html;
        }

}


# 小当互利微信搜索 
server {
        listen       80;
        server_name   xiaodang.m.duoquwawa.net lllmmm.m.duoquwawa.net yoyoquan.m.latl.site yoyoquan.m.ssaayy.cn;

        proxy_set_header Host $host;
        proxy_set_header X-Real-IP $remote_addr;
        proxy_set_header REMOTE-HOST $remote_addr;
        proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;

        location / {
          proxy_pass http://proxy.m.fqapps.com;
        }
}


server {
        listen       80 ;
        server_name     xiaodang.cai.dangqukeji.com ;

        proxy_set_header Host $host;
        proxy_set_header X-Real-IP $remote_addr;
        proxy_set_header REMOTE-HOST $remote_addr;
        proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;


        return 301 http://xiaodang.m.duoquwawa.net$request_uri;
       
 }



server {
    listen 80;
    server_name  keke.ssaayy.cn test.ssaayy.cn;
    location ~\.php$ {
            fastcgi_pass   127.0.0.1:9000;
            fastcgi_index  index.php;
            fastcgi_param  SCRIPT_FILENAME /usr/local/dataoke$fastcgi_script_name;
            include        fastcgi_params;
    }
    location / {
        root   /usr/local/dataoke;
        index  index.php;
    }
}




#web-h5
server {
        listen       80;
        server_name  web.duodianapp.cn web.huoyingcangbaoge.cn xiaodang.bbuutt.cn;

        root        /usr/local/web-h5/;
        index       index.html,index.htm;

        proxy_set_header Host $host;
        proxy_set_header X-Real-IP $remote_addr;
        proxy_set_header REMOTE-HOST $remote_addr;
        proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;
     
        location / {
            try_files $uri /index.html;
        }

}


